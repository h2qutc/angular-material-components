{
  "name": "angular-material-components",
  "version": "2.0.3",
  "description": "Angular Material Datetime Picker",
  "author": "HO Hong Quan",
  "repository": {
    "type": "git",
    "url": "https://github.com/h2qutc/angular-material-components.git"
  },
  "bugs": {
    "url": "https://github.com/h2qutc/angular-material-components/issues"
  },
  "homepage": "https://github.com/h2qutc/angular-material-components",
  "keywords": [
    "angular",
    "angular2",
    "angular-material",
    "angular material datetime picker",
    "angular material time picker",
    "datetime-picker",
    "datepicker",
    "timepicker",
    "ngx-mat",
    "ngx-mat-datetime-picker",
    "ngx-material-datetime-picker",
    "ngx-mat-timepicker"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codecov": "codecov",
    "build-lib": "ng build --configuration production datetime-picker",
    "npm-pack": "cd dist/datetime-picker && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publishLib": "cp -f README.md dist/datetime-picker && cp -f README.md projects/datetime-picker && cd dist/datetime-picker && npm publish",
    "XUX": "npm version minor && cd projects/datetime-picker && npm version minor",
    "XXU": "npm version patch && cd projects/datetime-picker && npm version patch",
    "BPP": "npm run package && npm run publishLib #BUILD PACKAGE n PUBLISH",
  },
  "private": false,
  "dependencies": {
    "@angular-material-components/moment-adapter": "^2.0.1",
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "~7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "8.2.13",
    "@types/highlight.js": "^9.12.3",
    "core-js": "^2.5.4",
    "highlight.js": "^9.18.1",
    "moment": "^2.24.0",
    "ngx-highlightjs": "^4.1.0-beta",
    "rxjs": "~6.3.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codecov": "^3.6.1",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
